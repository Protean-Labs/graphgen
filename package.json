{
  "name": "graphgen",
  "version": "0.1.0",
  "description": "Subgraph Generation Tool",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "pesy build",
    "release": {
      "rewritePrefix": true,
      "releasedBinaries": [
        "graphgen.exe"
      ]
    }
  },
  "pesy": {
    "azure-project": "esy-dev/esy",
    "github": "esy/pesy-reason-template"
  },
  "buildDirs": {
    "graphgen": {
      "rawBuildConfigFooter": [
        "(ocamllex (modules lexer))",
        "(menhir (modules parser) (infer false))"
      ],
      "bin": { 
        "graphgen.exe": "main.re"
      }  
    }
  },
  "scripts": {
    "graphgen": "esy x graphgen.exe",
    "test": "esy dune runtest --instrument-with bisect_ppx --force --profile release"
  },
  "dependencies": {
    "@esy-ocaml/reason": "^3.7.0",
    "@opam/dune": "^2.7.0",
    "@opam/logs": "*",
    "@opam/menhir": "*",
    "@opam/ocamlfind": "1.8.1",
    "@opam/ppx_deriving": "^4.5",
    "@opam/ppx_deriving_yojson": "^3.5.3",
    "@opam/yojson": "*",
    "@pesy/esy-pesy": "0.1.0-alpha.11",
    "@reason-native/console": "*",
    "fmt": "^2.0.0",
    "ocaml": "4.10.0"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "@opam/ocaml-lsp-server": "1.4.1",
    "refmterr": "*",
    "@opam/bisect_ppx": "^2.5.0",
    "@opam/utop": "*",
    "@opam/rtop": "*"
  }
}