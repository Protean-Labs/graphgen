{
  "name": "graphgen",
  "version": "0.1.0",
  "description": "Subgraph Generation Tool",
  "license": "MIT",
  "esy": {
    "build": "dune build -p graphgen",
    "buildDev": "refmterr dune build --promote-install-files --root . --only-package #{self.name}",
    "release": {
      "rewritePrefix": true,
      "releasedBinaries": [ "graphgen.exe" ]
    }
  },
  "scripts": {
    "graphgen": "esy x graphgen.exe",
    "test": "esy x test.exe",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cvauclair/graphgen.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
